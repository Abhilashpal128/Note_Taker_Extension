{
  "manifest_version": 3,
  "name": "Google Meet Audio Recorder",
  "version": "1.0",
  "description": "Record Google Meet audio and log in console.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://meet.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Google Meet Audio Recorder",
    "default_popup": "index.html",
    "default_icon": "logo192.png"
  }
}
